<h1>All Advertisments</h1>
<% @advertisement.each do |advertisement| %>
<div class="media">
  <div class="media-body">
    <h4 class="media-heading">
      <%= link_to advertisement.title, advertisement %>
      <%= link_to advertisement.copy, advertisement %>
      <%= link_to advertisement.price, advertisement %>
    </h4>
  </div>
</div>
<% end %>
